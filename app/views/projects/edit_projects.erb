<form action="/projects/:project_name/edit" method="POST">
  Project name: <%= @project.project_name%><br>
  Description: <%= @project.description%><br>

  Content: <input type="text" name="content" id="content"><br>
  <input type="submit" name="edit" id="edit" value="edit project"><br>
</form>

<form action="/projects/:project_name/delete" method="POST">
  <input type="submit" name="delete" id="delete" value="delete project">
</form>

